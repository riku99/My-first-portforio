<div class="login-wrapper">
  <div class="login-form">
    <%= form_for(:session, url: login_path) do |f| %>
    <div class="login">
      <%= f.label :acount_id, "User_id", class: "login-label" %>
      <%= f.text_field :acount_id, class: "login-field" %>
    </div>
    <div class="login">
      <%= f.label :password, class: "login-label" %>
      <%= f.password_field :password, class: "login-field" %>
    </div>
    <%= f.submit "Login", class: "login-button" %>
    <% end %>
  </div>
</div>
